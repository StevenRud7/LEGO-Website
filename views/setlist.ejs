<h1>The sets are</h1>
All sets matching the given filter
<hr>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Set Number</th>
            <th>Name</th>
            <th>Release Year</th>
            <th>Price</th>
            <th>Pieces</th>
            <th>Image</th>
        </tr>
    </thead>
    <tbody></tbody>
    <% for (set of sets) { %>
      <tr class="mb-2"> 
        <td><%= set['Set_Number'] %></td>

        <td>
            <strong><%= set['Name'] %></strong>
        </td>

        <td>
            <% if (set['Release_Year'] >1963) { %>
                <p> <%=set['Release_Year']%> </p>
                <%} else { %>
                <p> N/A </p>
            <% } %>
            
        </td>

        <td>
            <% if (set['Price'] >0) { %>
                <p> <%=set['Price']%> </p>
                <%} else { %>
                <p> N/A </p>
            <% } %>
        </td>
        

        <td>
            <% if (set['Pieces'] >0) { %>
                <p> <%=set['Pieces']%> </p>
                <%} else { %>
                <p> N/A </p>
            <% } %>
        </td>
        <td>
            <img src = "<%= set.Image%>" width = "30%" class="fit-picture">
        </td>
    </tr>
    <% } %>
    </tbody>
</table>